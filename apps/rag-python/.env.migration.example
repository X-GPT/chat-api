# ============================================================
# Migration Configuration
# ============================================================
# Add these environment variables to your main .env file
# (NOT a separate .env.migration file)
# The migration scripts will read from .env like the main app

# MySQL Configuration (AWS RDS)
MYSQL_HOST=your-rds-endpoint.region.rds.amazonaws.com
MYSQL_PORT=3306
MYSQL_USER=your_user
MYSQL_PASSWORD=your_password
MYSQL_DATABASE=your_database
MYSQL_TABLE=ip_summary
MYSQL_POOL_SIZE=10

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-service-role-key-here

# Migration Configuration
BATCH_SIZE=100
MAX_WORKERS=5
MAX_RETRIES=3
WORKER_POLL_INTERVAL=1.0
WORKER_HEARTBEAT_INTERVAL=30.0
BATCH_TIMEOUT_MINUTES=10
